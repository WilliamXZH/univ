package net.librec.similarity;

import java.util.List;

/**
 * Bayesian Similarity
 * @author William (Zenghui Xu)
 *
 */
public class BSSimilarity extends AbstractRecommenderSimilarity{

	
	/* (non-Javadoc)
	 * @see net.librec.similarity.AbstractRecommenderSimilarity#getSimilarity(java.util.List, java.util.List)
	 */
	@Override
	protected double getSimilarity(List<? extends Number> thisList, List<? extends Number> thatList) {
        if (thisList == null || thatList == null || thisList.size() < 1 || thatList.size() < 1 || thisList.size() != thatList.size()) {
            return Double.NaN;
        }
		
		// TODO Auto-generated method stub
		return 0;
	}
	
	
}
