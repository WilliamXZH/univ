}
/*订单处理总格局 跟踪，退款，快递详情等*/
#dealOrder{
	margin-top: 5px;
	margin-left: 5px;
	height: 25px;
}
/*订单处理样式*/
#dealOrder ul li{
	float: right;
	line-height: 24px;
	list-style: none;
}
#dealOrder ul li:first-child{
	float: left;
}
/*消除orderDeal中button原来样式*/
.dealOrder{
	background-color: white;
	border: none;
	margin-left: 5px;
}
/*对订单的选择样式*/
.dealOrder:hover {
	border: 1px solid deepskyblue;
	cursor: pointer;
	background-color: #97cbff;
	border-radius: 5px;
	opacity: 0.8;
}