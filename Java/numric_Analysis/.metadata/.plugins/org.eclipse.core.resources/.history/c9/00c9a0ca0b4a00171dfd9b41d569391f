package math.number.analysis.euler;

import org.junit.Test;

public class EulerTest {
	
	Euler euler = new Euler(0, 0, 2);
	
	@Test
	public void testEuler(){
		
		euler.setStepWith(0.1);
		euler.start();
		
		euler.setStepWith(0.2);
		euler.start();
		
		euler.setStepWith(0.4);
		euler.start();
	}
}
